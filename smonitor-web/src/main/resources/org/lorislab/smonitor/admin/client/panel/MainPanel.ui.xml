<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:s="urn:import:org.lorislab.smonitor.admin.client.navigation.panel"
             xmlns:a="urn:import:org.lorislab.smonitor.admin.client.app.panel"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">

    <g:SplitLayoutPanel ui:field="splitPanel">
        <g:west size="200">
            <s:NavigationPanel ui:field="navigationPanel" />
        </g:west>
        <g:center>
            <a:ApplicationPanel ui:field="appPanel"/>
        </g:center>
    </g:SplitLayoutPanel>
</ui:UiBinder>