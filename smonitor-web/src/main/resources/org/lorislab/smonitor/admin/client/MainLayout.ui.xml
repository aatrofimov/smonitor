        @external .button-view;
                    
        .arrow-popup, .arrow-popup-top, .arrow-popup-right, .arrow-popup-bottom, .arrow-popup-left {
        width: 50px;
        color:#ffffff;
        cursor:normal;
        display:-moz-inline-stack;
        display:inline-block;
        font-size:12px;
        <!--font-family:arial;-->
        <!--padding:.5em 1em;-->
        position:relative;
        text-align:center;
        text-shadow:0 -1px 1px #111111;
        -webkit-border-top-left-radius:4px ;
        -webkit-border-top-right-radius:4px ;
        -webkit-border-bottom-right-radius:4px ;
        -webkit-border-bottom-left-radius:4px ;
        -khtml-border-top-left-radius:4px ;
        -khtml-border-top-right-radius:4px ;
        -khtml-border-bottom-right-radius:4px ;
        -khtml-border-bottom-left-radius:4px ;
        -moz-border-radius-topleft:4px ;
        -moz-border-radius-topright:4px ;
        -moz-border-radius-bottomright:4px ;
        -moz-border-radius-bottomleft:4px ;
        border-top-left-radius:4px ;
        border-top-right-radius:4px ;
        border-bottom-right-radius:4px ;
        border-bottom-left-radius:4px ;
        -o-box-shadow:0 1px 2px #000000, inset 0 0 0 1px #FFFFFF, inset 0 2px #666666, inset 0 -2px 2px #444444;
        -moz-box-shadow:0 1px 2px #000000, inset 0 0 0 1px #FFFFFF, inset 0 2px #666666, inset 0 -2px 2px #444444;
        -khtml-box-shadow:0 1px 2px #000000, inset 0 0 0 1px #FFFFFF, inset 0 2px #666666, inset 0 -2px 2px #444444;
        -webkit-box-shadow:0 1px 2px #000000, inset 0 0 0 1px #FFFFFF, inset 0 2px #666666, inset 0 -2px 2px #444444;
        box-shadow:0 1px 2px #000000, inset 0 0 0 1px #FFFFFF, inset 0 2px #666666, inset 0 -2px 2px #444444;
        background-color:#3b3b3b;
        background-image:-moz-linear-gradient(top,#555555,#222222);
        background-image:-webkit-gradient(linear,left top,left bottom,color-stop(0,#555555),color-stop(1,#222222));
        }
        
        .arrow-popup:after, .arrow-popup-top:after, .arrow-popup-right:after, .arrow-popup-bottom:after, .arrow-popup-left:after {
        content:"\25B8";
        display:block;
        font-size:30px;
        height:0;
        line-height:0;
        position:absolute;
        }
        
        .arrow-popup:after, .arrow-popup-bottom:after {
        color:#2a2a2a;
        bottom:0;
        left:1px;
        text-align:center;
        text-shadow:1px 0 2px #000000;
        -o-transform:rotate(90deg);
        -moz-transform:rotate(90deg);
        -khtml-transform:rotate(90deg);
        -webkit-transform:rotate(90deg);
        width:100%;
        }
        
        .arrow-popup-top:after {
        bottom:auto;
        color:#4f4f4f;
        left:-2px;
        top:0;
        text-align:center;
        text-shadow:none;
        -o-transform:rotate(-90deg);
        -moz-transform:rotate(-90deg);
        -khtml-transform:rotate(-90deg);
        -webkit-transform:rotate(-90deg);
        width:100%;
        }
        
        .arrow-popup-right:after {
        color:#FFFFFF;
        right:-11px;
        top:50%;
        margin-top:-.05em;
        text-shadow:0 1px 2px #000000;
        -o-transform:rotate(0);
        -moz-transform:rotate(0);
        -khtml-transform:rotate(0);
        -webkit-transform:rotate(0);
        }
        
        .arrow-popup-left:after {
        color:#222222;
        left:-0.375em;
        top:50%;
        margin-top:.1em;
        text-shadow:0 -1px 2px #000000;
        -o-transform:rotate(180deg);
        -moz-transform:rotate(180deg);
        -khtml-transform:rotate(180deg);
        -webkit-transform:rotate(180deg);
        } 

        .arrow-popup-content {
        margin-top: 2px;
        }
                
        .arrow-popup-nativebutton {
        -webkit-appearance: none;
        display: block;
        padding: 10px 0px 10px;
        width: 100%;
        height: 10px;
        border: none;
        margin: 0;
        position: relative;
        border-top: 1px solid #4b4c4d;
        border-top-color: rgba(255, 255, 255, 0.05);
        border-bottom: 1px solid #353637;
        border-bottom-color: rgba(0, 0, 0, 0.18);
        background: transparent;
        color: #888a8c;
        font-weight: normal;
        text-shadow: 0 1px 0 rgba(0, 0, 0, 0.5);
        text-align: center;        
        }
        
        .arrow-popup-nativebutton:focus {
        outline: none;
        }

        .arrow-popup-nativebutton:hover, .arrow-popup-nativebutton:focus {
        color: #e1e1e1;
        }
   
        .arrow-popup-nativebutton-down,
        .arrow-popup-nativebutton-down-hovering,
        .arrow-popup-nativebutton-down-disabled        {
        color: #e1e1e1;
        text-shadow: 0 2px 2px rgba(0, 0, 0, 0.5);
        border-top-color: #262729;
        border-bottom-color: #2b2c2e;
        background: #333537;
        background: -moz-linear-gradient(top, #2d2f30 0%, #333537 10%, #333537 90%, #2d2f30 100%);
        background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #2d2f30), color-stop(10%, #333537), color-stop(90%, #333537), color-stop(100%, #2d2f30));
        background: -webkit-linear-gradient(top, #2d2f30 0%, #333537 10%, #333537 90%, #2d2f30 100%);
        background: -o-linear-gradient(top, #2d2f30 0%, #333537 10%, #333537 90%, #2d2f30 100%);
        background: -ms-linear-gradient(top, #2d2f30 0%, #333537 10%, #333537 90%, #2d2f30 100%);
        -webkit-transition: none;
        }
                
        @external .arrow-popup,.arrow-popup-right, .arrow-popup-content, .arrow-popup-nativebutton,.arrow-popup-nativebutton-down,.arrow-popup-nativebutton-down-hovering,.arrow-popup-nativebutton-down-disabled;   
        
        <!-- CHECKBOX -->
        .checkbox-basic input {
        display: none;
        }

        .checkbox-basic label {
        background-color: #FAFAFA;
        border: 1px solid #CACECE;
        border-radius: 3px 3px 3px 3px;
        box-shadow: 0 1px 2px rgba(0, 0, 0, 0.05), 0 -15px 10px -12px rgba(0, 0, 0, 0.05) inset;
        display: inline-block;
        padding: 9px;
        position: relative;
        }


        .checkbox-basic input:checked + label:after {
        color: #99A1A7;
        content: '\e80d';
        font-size: 15px;
        left: 4px;
        position: absolute;
        top: 0;
        font-family: "entypo";
        }
        .checkbox-basic input:checked + label {
        background-color: #E9ECEE;
        border: 1px solid #ADB8C0;
        color: #99A1A7;
        }
        
        @external .checkbox-basic;      
        
