    <ui:style>
        .gwt-Button {
        margin: 0;
        padding: 5px 7px;
        text-decoration: none;
        cursor: pointer;
        cursor: hand;
        font-size:small;
        background: url("images/hborder.png") repeat-x 0px -2077px;
        border:1px solid #bbb;
        border-bottom: 1px solid #a0a0a0;
        border-radius: 3px;
        -moz-border-radius: 3px;
        }
        .gwt-Button:active {
        border: 1px inset #ccc;
        }
        .gwt-Button:hover {
        border-color: #939393;
        }
        .gwt-Button[disabled] {
        cursor: default;
        color: #888;
        }
        .gwt-Button[disabled]:hover {
        border: 1px outset #ccc;
        }
     
        .content-center table thead tr th {
