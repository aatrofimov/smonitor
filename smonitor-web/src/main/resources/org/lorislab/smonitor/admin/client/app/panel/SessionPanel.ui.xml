<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:s="urn:import:org.lorislab.smonitor.admin.client.app.panel"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">

    <g:SplitLayoutPanel ui:field="splitPanel">
        <g:south size="20">
            <g:DockLayoutPanel unit="PX" ui:field="detailsVPanel">
                <g:north size="150">
                    <s:SessionDetailsPanel ui:field="sessionDetailsPanel" />
                </g:north>
                <g:center>
                    <s:AttributesPanel ui:field="attributesPanel" />
                </g:center>
            </g:DockLayoutPanel>            
        </g:south>
        <g:center>
            <s:SessionsTable ui:field="sessionTable" />
        </g:center>
    </g:SplitLayoutPanel>

</ui:UiBinder>