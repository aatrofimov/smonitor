<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:s="urn:import:org.lorislab.smonitor.admin.client.app.panel"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">


    <g:TabLayoutPanel ui:field="tabPanel" barHeight="2.5" barUnit="EM">
        <g:tab >
            <g:header>Details</g:header>
            <s:ApplicationDetailsPanel ui:field="applicationDetails" />
        </g:tab>                
        <g:tab>
            <g:header>Sessions</g:header>
            <s:SessionPanel ui:field="sessionPanel" />
        </g:tab>
    </g:TabLayoutPanel>            

</ui:UiBinder>